{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "connectionString": {
      "type": "string"
    }
  },
  "variables": {},
  "resources": [
    {
      "kind": "V2",
      "properties": {
        "displayName": "azureblob",
        "parameterValues": {
          "connectionString": "[parameters('connectionString')]"
        },
        "api": {
          "id": "[concat(subscription().id, '/providers/Microsoft.Web/locations/', resourceGroup().location, '/managedApis/', 'azureblob')]"
        }
      },
      "name": "azureblob",
      "type": "Microsoft.Web/connections",
      "location": "[resourceGroup().location]",
      "apiVersion": "2016-06-01"
    }
  ],
  "outputs": {
    "connectionRuntimeUrl": {
      "type": "string",
      "value": "[reference(resourceId('Microsoft.Web/connections', 'azureblob'),'2016-06-01', 'full').properties.connectionRuntimeUrl]"
    }
  }
}
