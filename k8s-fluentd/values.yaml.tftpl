# TODO
env:
- name: "FLUENTD_CONF"
  value: "../../../etc/fluent/fluent.conf"
- name: FLUENT_ELASTICSEARCH_HOST
  value: "elasticsearch-master"
- name: FLUENT_ELASTICSEARCH_PORT
  value: "9200"
fileConfigs:
  04_outputs.conf: |-
    <label @OUTPUT>
      <match **>
        @type opensearch
        host "opensearch-master"
        port 9200
        path ""
        user opensearch
        password ${opensearch_password}
      </match>
    </label>